{
    "name": "supportplatform-service",
    "version": "1.0.0",
    "description": "SupportPlatform service for GitHub Project Evaluation Support Platform",
    "author": "Lucas Ângelo Oliveira Martins Rocha (https://lucasangelo.com/)",
    "main": "src/index.ts",
    "scripts": {
        "lint:check": "npx eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
        "lint:watch": "nodemon -x \"npm run lint:check\"",
        "lint:fix": "npx eslint . --fix --ext .ts --report-unused-disable-directives --max-warnings 0",
        "test": "jest",
        "start": "npx ts-node index.ts",
        "start:dev": "ts-node-dev index.ts",
        "build": "npm run swagger && npx tsc --outDir dist",
        "start:builded": "npx ts-node --transpile-only dist/index.js",
        "start:prod": "npm run build && npm run start:builded",
        "dev": "concurrently \"npm run start:dev\" \"npm run swagger:watch\" \"npm run lint:watch\"",
        "swagger": "tsoa spec-and-routes",
        "swagger:watch": "nodemon -x tsoa spec-and-routes",
        "prettify": "npx prettier -w .",
        "prettify:check": "npx prettier -c ."
    },
    "keywords": [
        "nodejs",
        "dotenv",
        "editorconfig",
        "mysql"
    ],
    "license": "CC-BY-4.0",
    "dependencies": {
        "@gitgrade/dtos": "file:../dtos-lib",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mysql2": "^3.1.2",
        "sequelize": "^6.29.0",
        "swagger-ui-express": "^5.0.0",
        "tsoa": "^5.1.1",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.4",
        "@types/sequelize": "^4.28.15",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.6.3",
        "nodemon": "^3.0.1",
        "prettier": "^2.8.4",
        "sequelize-cli": "^6.6.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsc-files": "^1.1.4",
        "typescript": "^5.1.6"
    },
    "homepage": "https://github.com/ICEI-PUC-Minas-PPLES-TI/plf-es-2023-1-tcci-0393100-dev-plataformaapoioavaliacoesprojetos",
    "bugs": "https://github.com/ICEI-PUC-Minas-PPLES-TI/plf-es-2023-1-tcci-0393100-dev-plataformaapoioavaliacoesprojetos/issues",
    "repository": {
