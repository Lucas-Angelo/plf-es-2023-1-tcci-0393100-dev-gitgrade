        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "concurrently": "^8.2.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.6.3",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.2",
        "sequelize-cli": "^6.6.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsc-files": "^1.1.4",
        "typescript": "^5.2.2"
    },
