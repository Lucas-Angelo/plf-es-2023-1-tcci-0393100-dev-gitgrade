{
    "name": "supportplatform-service",
    "version": "1.0.0",
    "description": "SupportPlatform service for GitHub Project Evaluation Support Platform",
    "author": "Lucas Ângelo Oliveira Martins Rocha (https://lucasangelo.com/)",
    "main": "src/index.ts",
    "scripts": {
        "lint:check": "npx eslint --ext .js ./",
        "lint:fix": "npx eslint --fix --ext .js ./",
        "test": "echo \"Running all tests\"",
        "start": "npx ts-node src/index.ts",
        "start:dev": "npm run dev",
        "build": "npx run swagger && npx tsc --outDir dist",
        "start:builded": "npx ts-node --transpile-only dist/index.js",
        "start:prod": "npm run build && npm run start:builded",
        "dev": "concurrently \"ts-node-dev src/index.ts\" \"nodemon -x tsoa spec-and-routes\"",
        "swagger": "tsoa spec-and-routes"
    },
    "keywords": [
        "nodejs",
        "dotenv",
        "editorconfig",
        "mysql"
    ],
    "license": "CC-BY-4.0",
    "dependencies": {
        "@gitgrade/dtos": "file:../dtos-lib",
        "@gitgrade/models": "file:../models-lib",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mysql2": "^3.1.2",
        "sequelize": "^6.29.0",
        "swagger-ui-express": "^5.0.0",
        "tsoa": "^5.1.1",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/sequelize": "^4.28.15",
        "@types/swagger-ui-express": "^4.1.3",
        "concurrently": "^8.2.0",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^3.0.1",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    },
